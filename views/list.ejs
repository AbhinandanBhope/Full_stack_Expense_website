<!DOCTYPE html>
<html>
  <head>
    <title>User List</title>
  </head>
  <body>
    <a href="/addUser" class="btn" >Add Expense</a>
    <h1>Expense List</h1>
    <% rows.forEach((row) => { %>
      <p>ID: <%= row.id %> - Title: <%= row.title %> - Cost: <%= row.price %> </p>
    
      <a href="/delete/<%= row.id %>" class="btn" >Delete</a>
    <% }); %>
  </body>
</html>
